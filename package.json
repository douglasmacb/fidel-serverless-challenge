{
  "name": "be-techtest-douglasmiranda",
  "version": "1.0.0",
  "description": "Fidel Coding Challenge",
  "main": "serverless.ts",
  "scripts": {
    "dotenv": "STAGE=${STAGE:=dev} && sls export-env -s $STAGE",
    "test": "npm run dotenv && jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:staged": "npm test -- --findRelatedTests ./src/**/**/*",
    "deploy": "sls deploy",
    "dev": "sls offline start",
    "dynamo:up": "sls dynamodb start",
    "dynamo:down": "sls dynamodb stop",
    "test:ci": "npm test -- --coverage"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1069.0",
    "serverless-offline-sqs": "^6.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.49.0",
    "@serverless/typescript": "^3.0.0",
    "@shelf/jest-dynamodb": "^2.2.3",
    "@types/aws-lambda": "^8.10.71",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.11",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^4.2.5",
    "jest": "^27.5.0",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^10.2.2",
    "serverless": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-export-env": "github:arabold/serverless-export-env",
    "serverless-offline": "^8.4.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "author": "Douglas Miranda <douglasmacbrito@gmail.com>",
  "license": "MIT"
}
